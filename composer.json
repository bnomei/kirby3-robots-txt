{
    "name": "bnomei/kirby3-robots-txt",
    "type": "plugin",
    "description": "Manage the robots.txt from the Kirby config file",
    "version": "1.1.1",
    "license": "MIT",
    "authors": [
        {
            "name": "Bruno Meilick",
            "email": "b@bnomei.com"
        }
    ],
    "keywords": [
        "kirby3",
        "kirby3-cms",
        "kirby3-plugin",
        "robots-txt",
        "robots-exclusion-protocol"
    ],
    "suggest": {
    },
    "autoload": {
        "files": [
            "config.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1.0",
        "getkirby/cms": "^3.0.0-RC-2.0"
    },
    "conflict": {
        "getkirby/kirby": "<3.0"
    },
    "scripts": {
        "zip": [
            "touch kirby3-robots-txt.zip",
            "rm kirby3-robots-txt.zip",
            "composer install --no-dev",
            "composer remove getkirby/cms",
            "composer dumpautoload -o",
            "zip -r kirby3-robots-txt.zip . -x *.git*",
            "composer require getkirby/cms:'^3.0.0-RC-2.0'"
        ]
    }
}
